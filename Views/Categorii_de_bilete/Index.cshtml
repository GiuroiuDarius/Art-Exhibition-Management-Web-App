@model IEnumerable<test3.Models.Categorii_de_bilete>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Denumire)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pret)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Denumire)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pret)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Categorie_bilet_ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Categorie_bilet_ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Categorie_bilet_ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
