@model IEnumerable<test3.Models.Exponate>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nume_exponat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descriere)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.An_fabricatie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExpozitieID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ArtistID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CategorieID)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nume_exponat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descriere)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.An_fabricatie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExpozitieID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ArtistID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CategorieID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ExponatId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ExponatId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ExponatId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
